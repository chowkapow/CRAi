{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/bin/www",
    "start": "npm run build && node ./build/bin/www",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src -s",
    "clean": "rm -rf ./build && mkdir build"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.18.2",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "@babel/node": "^7.7.4",
    "nodemon": "^1.19.4"
  }
}
